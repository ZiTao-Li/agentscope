                  "init_args": {
                    "language": "python",
                    "chunk_lines": 100
                  }
                }
              ]
            },
            "chunk_size": 2048,
            "chunk_overlap": 40,
            "similarity_top_k": 10,
            "log_retrieval": false,
            "recent_n_mem": 1
      }
    }
  },
